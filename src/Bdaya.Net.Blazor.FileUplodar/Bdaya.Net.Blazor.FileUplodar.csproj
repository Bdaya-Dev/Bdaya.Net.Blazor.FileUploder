<Project Sdk="Microsoft.NET.Sdk.Razor">

	<PropertyGroup>
		<TargetFramework>net5.0</TargetFramework>
		<PackageIcon>Bdaya.png</PackageIcon>
		<PackageId>Bdaya.Net.Blazor.FileUploader</PackageId>
		<Version>1.0.2</Version>
		<Authors>AhmedKhalil777</Authors>
		<Company>Bdaya-Dev</Company>
		<PackageDescription>A razor library for uploading files</PackageDescription>
		<RepositoryUrl>https://github.com/Bdaya-Dev/Bdaya.Net.Blazor.FileUploder</RepositoryUrl>
